{
	"name": "console-helpers/process-iterator",
	"description": "Sequential way to run Symfony Processes in parallel",
	"keywords": ["symfony", "process", "iterator", "parallel"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Alexander Obuhovich",
			"email": "aik.bold@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.7",
		"symfony/process": "^2.7|^3.0"
	},
	"require-dev": {
		"aik099/coding-standard": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"ConsoleHelpers\\ProcessIterator\\": "src/ProcessIterator/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\ConsoleHelpers\\ProcessIterator\\": "tests/ProcessIterator/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
